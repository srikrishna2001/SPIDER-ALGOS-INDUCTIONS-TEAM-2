binaryadd(char a[],int x)
{   
    int i;
    i=x-1;
	if(a[i]=='0')
	{
		a[i]='1';
	}
	else
	{
		a[i]='0';
		x--;
		binaryadd(a,x);
	}
}
binarysub(char a[],int x)
{
